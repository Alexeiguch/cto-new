# Environment variables for Real Estate Workflow Platform
# Copy this file to .env.local and fill in your actual values

# API Keys
OPENAI_API_KEY=sk_test_your_openai_key_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/real_estate_db

# Optional: Cloud Storage Configuration
# AWS S3
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_S3_BUCKET=your_bucket_name
AWS_REGION=us-east-1

# Optional: Environment
NODE_ENV=development
